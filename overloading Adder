import java.io.*;
public class Adder {
    int add(int a,int b) {
        return a+b;
    }
    int add(int a,int b,int c) {
        return a+b+c;
    }
    int add(int a) {
        return 0+a;
    }
}
class MethodOverLoadingDemo1 {
    public static void main(String args[]) {
        Adder ad = new();
        int sc=ad.add(5);
        int y=ad.add(10,20);
        int z=ad.add(x,y,o);
        System.out.println("values of x is"+x);
        System.out.println("values of x is"+y);
        System.out.println("values of x is"+z);
    }
}
